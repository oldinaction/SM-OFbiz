<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>aezo Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="noSyntaxError" engine="interface">
		<description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
	</service>

	<service name="createSmPersonOfTest" engine="simple" default-entity-name="SmPerson" auth="false"
		invoke="createSmPersonOfTest" location="component://aezo/script/cn/aezo/test/TestServices.xml">
		<description>ofbiz Test：产生一条SmPerson记录</description>
		<attribute name="username" mode="IN" type="String"/>
		<attribute name="password" mode="IN" type="String"/>
		<attribute name="description" mode="IN" type="String" optional="true"/>
		<attribute name="id" mode="OUT" type="String"></attribute>
	</service>
	<service name="createSmPersonOfTestJava" engine="java" default-entity-name="SmPerson" auth="false"
		invoke="createSmPersonOfTestJava" location="cn.aezo.mytest.Test">
		<description>ofbiz Test：产生一条SmPerson记录</description>
		<attribute name="username" mode="IN" type="String"/>
		<attribute name="password" mode="IN" type="String"/>
		<attribute name="description" mode="IN" type="String" optional="true"/>
		<attribute name="flag" mode="OUT" type="String"></attribute>
	</service>
	

</services>