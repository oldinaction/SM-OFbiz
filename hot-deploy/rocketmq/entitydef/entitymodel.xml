<?xml version="1.0" encoding="UTF-8"?>
<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of RocketMQ Component</title>
    <description>None</description>
    <copyright></copyright>
    <version></version>
    
    <entity entity-name="SmPerson" no-auto-stamp="true" package-name="cn.aezo.rocketmq">
    	<field name="personId" type="id"/>
    	<field name="username" type="short-varchar"/>
    	<field name="password" type="short-varchar"/>
    	<field name="description" type="short-varchar"/>
    	<field name="inputTm" type="date-time"/>
    	<field name="updateTm" type="date-time"/>
    	<prim-key field="personId"/>
    </entity>
    
    <entity entity-name="SmPersonContact" no-auto-stamp="true" package-name="cn.aezo.rocketmq">
    	<field name="contactId" type="id"/>
    	<field name="personId" type="id"/>
    	<field name="telephone" type="short-varchar"/>
    	<field name="address" type="short-varchar"/>
    	<field name="inputTm" type="date-time"/>
    	<field name="updateTm" type="date-time"/>
    	<prim-key field="contactId"/>
    </entity>

</entitymodel>